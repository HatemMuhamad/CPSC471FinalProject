<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gymmanagmentsystem">


    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".OngoingSessions"></activity>
        <activity android:name=".Receipts" />
        <activity android:name=".MembershipInfo" />
        <activity android:name=".editAccount" />
        <activity android:name=".Account" />
        <activity android:name=".LoginView" />
        <activity android:name=".CreateAccountView" />
        <activity android:name=".TrainerViewSessionsView" />
        <activity android:name=".GymInfo" />
        <activity android:name=".TrainersSessions" />
        <activity android:name=".MemberPage" />
        <activity android:name=".TrainerPage" />
        <activity android:name=".MainPageView">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>