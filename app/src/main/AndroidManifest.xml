<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gymmanagmentsystem">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".View.MemberViewReceiptView"></activity>
        <activity android:name=".View.MemberViewMembershipInfoView" />
        <activity android:name=".View.TrainerEditAccountInfoView" />
        <activity android:name=".View.TrainerViewAccountInfoView" />
        <activity android:name=".View.LoginView" />
        <activity android:name=".View.CreateAccountView" />
        <activity android:name=".View.TrainerViewSessionsView" />
        <activity android:name=".View.TrainerViewGymInfoView" />
        <activity android:name=".View.TrainerViewSessionInfoView" />
        <activity android:name=".View.MemberMainPageView" />
        <activity android:name=".View.TrainerMainPageView" />
        <activity android:name=".View.MainPageView">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>